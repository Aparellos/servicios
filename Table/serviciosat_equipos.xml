<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : serviciosat_estados.xml
    Description:
        Estructura de la tabla serviciosat_equipos.
-->
<table>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>codfabricante</name>
        <type>character varying(8)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>text</type>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
    </column>
    <column>
        <name>idequipo</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>numserie</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>referencia</name>
        <type>character varying(30)</type>
    </column>
    <constraint>
        <name>serviciosat_equipos_pkey</name>
        <type>PRIMARY KEY (idequipo)</type>
    </constraint>
    <constraint>
        <name>ca_serviciosat_equipos_clientes</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_serviciosat_equipos_fabricantes</name>
        <type>FOREIGN KEY (codfabricante) REFERENCES fabricantes (codfabricante) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>