<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : nservicioscli.xml
    Description:
        Estructura de la tabla nservicioscli.
-->
<table>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>codalmacen</name>
        <type>character varying(4)</type>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>text</type>
    </column>
    <column>
        <name>editable</name>
        <type>boolean</type>
        <null>NO</null>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>NO</null>
    </column>
    <column>
        <name>fechafin</name>
        <type>date</type>
    </column>
    <column>
        <name>fechainicio</name>
        <type>date</type>
    </column>
    <column>
        <name>hora</name>
        <type>time without time zone</type>
    </column>
    <column>
        <name>horafin</name>
        <type>time without time zone</type>
    </column>
    <column>
        <name>horainicio</name>
        <type>time without time zone</type>
    </column>
    <column>
        <name>idempresa</name>
        <type>integer</type>
    </column>
    <column>
        <name>idestado</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>idservicio</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>nick</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>observaciones</name>
        <type>text</type>
    </column>
    <column>
        <name>prioridad</name>
        <type>integer</type>
    </column>
    <constraint>
        <name>nservicioscli_pkey</name>
        <type>PRIMARY KEY (idservicio)</type>
    </constraint>
    <constraint>
        <name>ca_nservicioscli_clientes</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_nservicioscli_empresas</name>
        <type>FOREIGN KEY (idempresa) REFERENCES empresas (idempresa) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_nservicioscli_estados</name>
        <type>FOREIGN KEY (idestado) REFERENCES nservicioscli_estados (id) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_nservicioscli_users</name>
        <type>FOREIGN KEY (nick) REFERENCES users (nick) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>